pipeline{
    agent {  label 'tox'
    }
    stages{
        stage('clone'){
            steps{
                   git url: 'https://github.com/rohi3369/shellscript.git',
                     branch: 'main'
        }
        }
        stage('build'){
           steps{ 
            sh 
           'aws ec2 create-vpc --cidr-block '10.0.0.0/16' --tag-specification 'ResourceType=vpc,Tags=[{Key=Name,Value=shellVpc}]' --region 'us-east-1')" 
    echo  "VPC ID '$VPC_ID' CREATED in '$AWS_REGION' region"'

    
    }
  }
}