pipeline{
    agent {label 'python'
    }
    stages{
        stage('clone'){
            steps{
                   git url: 'https://github.com/rohi3369/shellscript.git',
                     branch: 'main'
        }
        }
        stage('build'){
           steps{ 
            sh 'awscli.sh'
        }
    }
    }
}