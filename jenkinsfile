pipeline{
    agent {label 'python'
    }
    stages{
        stage('clone'){
            steps{
            git branch: 'main'
            url: 'https://github.com/rohi3369/shellscript.git'
        }
        }
        stage('build'){
           steps{ 
            sh 'awscli.sh'
        }
    }
    }
}